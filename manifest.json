{
    "name": "Snake",
    "description": "lightweight snake chrome extension!",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "/img/icon_16.png",
            "32": "/img/icon_32.png",
            "48": "/img/icon_48.png",
            "128": "/img/icon_128.png"
        },
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}